{
  "entryPoints": ["src/app.ts", "src/models", "src/controllers", "src/middleware", "src/utils"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": ["**/node_modules/**", "**/__tests__/**"],
  "tsconfig": "tsconfig.json",
  "includeVersion": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "theme": "default",
  "readme": "README.md"
}
